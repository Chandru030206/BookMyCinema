# BookMyCinema Backend Environment Variables
# Copy this file to .env and fill in your values

# Server Configuration
PORT=5000
NODE_ENV=development

# Database Configuration (MySQL)
DB_HOST=localhost
DB_PORT=3306
DB_USER=root
DB_PASSWORD=your_mysql_password
DB_NAME=bookmycinema_db

# JWT Configuration
JWT_SECRET=bookmycinema_super_secret_jwt_key_2024
JWT_EXPIRES_IN=7d

# Frontend URL (for CORS)
FRONTEND_URL=http://localhost:3000
